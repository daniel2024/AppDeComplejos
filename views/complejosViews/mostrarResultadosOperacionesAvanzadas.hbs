

<div class="row">&nbsp;</div>
<div class="row">&nbsp;</div>

<div class="row">

    <div class="col-md-6 offset-md-3">
        <div class="card">
            <div class="card-body">
                  <center><h5>Resultado de la Operación Avanzada</h5></center>              
<br>
                <form action="/operacionesAvanzadas"  name="panel" >
                <div id="items">
                     {{#each resultado.resultados}}




                    <div id="item_">
                        {{#if this.w}}
                           <label for="">{{this.w}}</label>
                        {{else}}
                            <label for="">{{this.w}}</label>
                        {{/if }}
                        <div class="form-group" id="bin_">
                            En Forma Binomica: (Re: <input style="text-align: right" size="5" type="text" id="real_" name="real_" value="{{this.z.real}}" />

                            Im: <input style="text-align: right" size="5" type="text" id="imag_" name="imag_" value="{{ this.z.imaginario }}" /> )

                        </div>
                        <div class="form-group" id="pol_">
                        En Forma Polar: [Mod: <input style="text-align: right" size="5" type="text" id="modulo_" name="modulo_" value="{{ this.z.mod }}"/>
                        
                        Arg: <input style="text-align: right" size="5" type="text" id="angle_" name="angle_" value="{{ this.z.angle }}"/> &Pi; ]

                        (Arg: <input style="text-align: right" size="4" type="text" id="radian_" name="radian_" readonly="readonly" value="{{this.grado}}"/>°)
                        </div>
                        {{#if this.primitiva}}
                            <div class="form-group" id="bin_">
                                Es Primitiva?: {{this.primitiva}}.
                            </div>
                        {{else}}
                        {{/if }}
                    </div>
                    {{/each}}
                </div>
                        <div>
                            <button type="submit" class="btn btn-success btn-block">atras</button>

                        </div>

                    </div>

                </form>


            </div>

        </div>

    </div>


</div>